{
  "name": "bullmq-explorer",
  "displayName": "BullMQ Explorer",
  "description": "VS Code extension for managing BullMQ queues",
  "version": "1.0.0",
  "publisher": "tipfurion",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tipfurion/vscode-bullmq"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bullmq-explorer",
          "title": "BullMQ Explorer",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "bullmq-explorer": [
        {
          "id": "bullmq-explorer",
          "name": "Queues"
        }
      ]
    },
    "commands": [
      {
        "command": "bullmq-explorer.showConfig",
        "title": "bullmq-explorer: Show Configuration"
      },
      {
        "command": "bullmq-explorer.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "bullmq-explorer.refreshConnection",
        "title": "Refresh Connection",
        "icon": "$(refresh)"
      },
      {
        "command": "bullmq-explorer.refreshQueue",
        "title": "Refresh Queue",
        "icon": "$(refresh)"
      },
      {
        "command": "bullmq-explorer.filter",
        "title": "Filter..."
      },
      {
        "command": "bullmq-explorer.clearFilter",
        "title": "Clear Filter"
      },
      {
        "command": "bullmq-explorer.showFilterMenu",
        "title": "Manage Filter"
      },
      {
        "command": "bullmq-explorer.sort",
        "title": "Sort..."
      },
      {
        "command": "bullmq-explorer.clearSort",
        "title": "Clear Sort"
      },
      {
        "command": "bullmq-explorer.showSortMenu",
        "title": "Manage Sort"
      },
      {
        "command": "bullmq-explorer.manageConnections",
        "title": "Manage Connections"
      },
      {
        "command": "bullmq-explorer.createJob",
        "title": "Create Job"
      },
      {
        "command": "bullmq-explorer.drainQueue",
        "title": "Drain Queue"
      },
      {
        "command": "bullmq-explorer.obliterateQueue",
        "title": "Obliterate Queue"
      },
      {
        "command": "bullmq-explorer.showJob",
        "title": "Show Job Details"
      },
      {
        "command": "bullmq-explorer.editJob",
        "title": "Edit Job"
      },
      {
        "command": "bullmq-explorer.removeJob",
        "title": "Remove Job"
      },
      {
        "command": "bullmq-explorer.promoteJob",
        "title": "Promote Job"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "bullmq-explorer.refresh",
          "when": "view == bullmq-explorer",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "bullmq-explorer.refreshConnection",
          "when": "view == bullmq-explorer && viewItem == connection",
          "group": "inline"
        },
        {
          "command": "bullmq-explorer.refreshQueue",
          "when": "view == bullmq-explorer && viewItem == queue",
          "group": "inline"
        },
        {
          "command": "bullmq-explorer.createJob",
          "when": "view == bullmq-explorer && viewItem == queue",
          "group": "1_queue"
        },
        {
          "command": "bullmq-explorer.drainQueue",
          "when": "view == bullmq-explorer && viewItem == queue",
          "group": "1_queue"
        },
        {
          "command": "bullmq-explorer.obliterateQueue",
          "when": "view == bullmq-explorer && viewItem == queue",
          "group": "1_queue"
        },
        {
          "command": "bullmq-explorer.showJob",
          "when": "view == bullmq-explorer && viewItem == job",
          "group": "1_job"
        },
        {
          "command": "bullmq-explorer.showJob",
          "when": "view == bullmq-explorer && viewItem == job.delayed",
          "group": "1_job"
        },
        {
          "command": "bullmq-explorer.editJob",
          "when": "view == bullmq-explorer && viewItem == job",
          "group": "1_job"
        },
        {
          "command": "bullmq-explorer.editJob",
          "when": "view == bullmq-explorer && viewItem == job.delayed",
          "group": "1_job"
        },
        {
          "command": "bullmq-explorer.promoteJob",
          "when": "view == bullmq-explorer && viewItem == job.delayed",
          "group": "1_job"
        },
        {
          "command": "bullmq-explorer.removeJob",
          "when": "view == bullmq-explorer && viewItem == job",
          "group": "1_job"
        },
        {
          "command": "bullmq-explorer.removeJob",
          "when": "view == bullmq-explorer && viewItem == job.delayed",
          "group": "1_job"
        },
        {
          "command": "bullmq-explorer.showFilterMenu",
          "when": "view == bullmq-explorer && viewItem == filter",
          "group": "1_filter"
        },
        {
          "command": "bullmq-explorer.clearFilter",
          "when": "view == bullmq-explorer && viewItem == filter",
          "group": "1_filter"
        },
        {
          "command": "bullmq-explorer.showSortMenu",
          "when": "view == bullmq-explorer && viewItem == sort",
          "group": "1_sort"
        },
        {
          "command": "bullmq-explorer.clearSort",
          "when": "view == bullmq-explorer && viewItem == sort",
          "group": "1_sort"
        },
        {
          "command": "bullmq-explorer.manageConnections",
          "when": "view == bullmq-explorer && viewItem == manageConnections",
          "group": "1_connections"
        }
      ]
    },
    "configuration": {
      "title": "BullMQ Explorer Configuration",
      "properties": {
        "bullmq-explorer.connections": {
          "type": "array",
          "default": [
            {
              "name": "My firt connection",
              "config": {
                "host": "localhost",
                "port": 6379,
                "password": ""
              }
            }
          ],
          "description": "Redis connections"
        }
      }
    }
  },
  "scripts": {
    "seed": "tsx scripts/seed.ts",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@faker-js/faker": "^10.1.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "^22",
    "@types/vscode": "^1.100.0",
    "eslint": "^9.13.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "dependencies": {
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.0",
    "jsonc-parser": "^0.4.2"
  }
}
